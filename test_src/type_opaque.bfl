type alias opaque UserId = int
namespace UserId {
  // TODO: The type equivalence int == UserId is available
  //      only in this namespace.
  // So we can write constructors
  fn make(value: int): UserId {
    value
  }

  fn toInt(self: UserId): int {
    self
  }

}

fn main(): int {
  val user_id: UserId = UserId::make(11);
  assert(user_id.toInt() == 11);
  0
}
