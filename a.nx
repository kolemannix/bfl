type Point = {
  x: int,
  y: int,
  z: int,
}

fn main(): int {
  val xs = [{ x: 1, y: 2, z: 3}];
  printInt(xs[0].x);
  printInt(xs[0].y);
  printInt(xs[0].z);
  mut i = 0;
  while i < 3 {
    print("pushing");
    xs.push<Point>({ x: 1, y: 2, z: 3});
    i = i + 1;
  };
  printInt(xs.capacity<Point>());
  printInt(xs.length<Point>());
  0
}