fn check_point(point: { x: int, y: int }): string {
  when point {
    { x: 1, y: 2 } -> "hi",
    _ -> "bye bye bye!"
  }
}
fn new_fn(): unit { () }
fn main(): int {
  val point = { x: 1, y: 2 };
  val s = check_point(point);
  val s2 = check_point({ x: 3, y: 4 });

  println(s);
  println(s2);

